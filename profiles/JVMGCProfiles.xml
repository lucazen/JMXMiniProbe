Please note, the apache.commons.digester3 is * VERY * case sensitive
########################################################### -->
<profiles xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Profile.xsd">
    <profile>
        <name>JVM GC Stats MarkSweep</name>
        <tag>jmxsensor,mjjmxsensor</tag>
        <kind>mjjmxgcmssensor</kind>
        <description>Garbage Collection statistics</description>
        <baseclass>com.paessler.prtg.jmx.sensors.JMXSensor</baseclass>
        <help>JVM Mark and Sweep Garbage Collection metrics</help>
        <entries>
            <entry>
                <object>java.lang:type=GarbageCollector,name=ConcurrentMarkSweep</object>
                <displayvalue>MS GarbageCollector Stats</displayvalue>
                <description>Mark and Sweap Garbage Collector</description>
                <shortdescription>GC-MS</shortdescription>
                <comment>Class Name:sun.management.GarbageCollectorImpl</comment>
                <attributes>
                    <!-- This is the number of garbage collection events fired since the JVM was launched.-->
                    <attribute>
                        <object>CollectionCount</object>
                        <displayvalue/>
                        <description>CollectionCount</description>
                        <shortdescription/>
                        <comment>type=long,unit=COUNT; Value=0</comment>
                        <unit>COUNT</unit>
                        <customunit/>
                        <enabled>true</enabled>
                    </attribute>
                    <!--CollectionTime is the total time spent doing garbage collection in milliseconds and is another good candidate to profile your Java application behaviour.-->
                    <attribute>
                        <object>CollectionTime</object>
                        <displayvalue/>
                        <description>CollectionTime</description>
                        <shortdescription/>
                        <comment>type=long,unit=TIME_RESPONSE; Value=0</comment>
                        <unit>TIME_RESPONSE</unit>
                        <customunit/>
                        <enabled>true</enabled>
                    </attribute>
                    <!-- Number of threads that performed the GC -->
                    <attribute>
                        <object>LastGcInfo.GcThreadCount</object>
                        <displayvalue/>
                        <description>LastGcInfo.GcThreadCount</description>
                        <shortdescription/>
                        <comment>type=javax.management.openmbean.CompositeData,unit=COUNT</comment>
                        <unit>COUNT</unit>
                        <customunit/>
                        <enabled>true</enabled>
                    </attribute>
                    <!-- Total duration of the GC event -->
                    <attribute>
                        <object>LastGcInfo.duration</object>
                        <displayvalue/>
                        <description>LastGcInfo.duration</description>
                        <shortdescription/>
                        <comment>type=javax.management.openmbean.CompositeData,unit=TIME_RESPONSE</comment>
                        <unit>TIME_RESPONSE</unit>
                        <customunit/>
                        <enabled>true</enabled>
                    </attribute>
                </attributes>
            </entry>
        </entries>
    </profile>
